<script type="text/javascript">

function checkNum(obj){
	var gopage=$(obj).val();
	 var reg = new RegExp("^[0-9]*$");  
	 if(!reg.test(gopage)){  
        $(obj).val("");
        return false;
     }  
     var _size=278;
     if(gopage>_size){
      $(obj).val(_size);
     }
}

function queryData(opt,obj){
	var url=$("#pageToUrl").val();
	var _form=$("#queryFormId").val();
	var currentPageNumber=Number($("#currentPageNumber").val());//当前页
	var currentPageCount=Number($("#currentPageCount").val());//每页显示条数
	var totalPageNum=$("#totalPageNum").val();//总页数
	
	var pageOffset=0;
	if(opt=="1"){//下一页操作
		pageOffset=currentPageNumber*currentPageCount;
	}else if(opt=="0"){//上一页
		pageOffset=0;
		if(currentPageNumber-1<=1){
			pageOffset=0;
		}else{
			pageOffset=(currentPageNumber-2)*currentPageCount;
		}
	}else if(opt=="3"){//尾页
		pageOffset=(totalPageNum-1)*currentPageCount;
	}else if(opt=="2"){//首页
		pageOffset=0;
	}else if(opt=="4"){//跳转页
	  var pNo = $("#topageNo").val();
	  if(pNo=="" || pNo==null || pNo=="undefined"){
	  	return false;
	  }
	  pageOffset=(pNo-1)*10;
	}else if(opt=="5"){//点击页码
		var pn=$(obj).attr("pageNo");
		if(pn-1<=0){
			pageOffset=0;
		}else{
			pageOffset=(pn-1)*currentPageCount;
		}
	}
	
	$("#pagerOffset").val(pageOffset);
	
	$(_form).prop("action",url);
	$(_form).submit();
	$.blockUI({ message: "数据查询中，请稍候...",css: { 
        border: 'none', 
        padding: '15px', 
        backgroundColor: '#000', 
        '-webkit-border-radius': '10px', 
        '-moz-border-radius': '10px', 
        opacity: .5, 
        color: '#fff' 
    } });
}

</script>
	<!-- 分页标签 1-->
	<div class="pageDiv">
					<input type="hidden" name="currentPageNumber" id="currentPageNumber" value="278" />
					<input type="hidden" name="currentPageCount" id="currentPageCount" value="10" />
					<input type="hidden" name="totalPageNum" id="totalPageNum" value="278" />
					<input type="hidden" name="totalNum" id="totalNum" value="2778" />
					<label class="">共2778条，当前第:278/278页</label>
					<a href="#" onclick="queryData('2');" class="pageLink">首页</a>
				<a href="#" onclick="queryData('0');" class="pageLink">上一页</a>
	                <a href="#" onClick="queryData('5',this);" pageNo="275" class="pageLink">275</a>
	                <a href="#" onClick="queryData('5',this);" pageNo="276" class="pageLink">276</a>
	                <a href="#" onClick="queryData('5',this);" pageNo="277" class="pageLink">277</a>
                <span class="selectPageLink">278</span>
				<a href="#" onclick="queryData('3');" class="pageLink">尾页</a>
		   		  转到
		      	<input id="topageNo" name="topageNo" onkeyup="checkNum(this);" class="page-txt" type="text" value />页
